<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>ABAIC · Welcome</title>
  <!-- Bootstrap -->
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
  <link href="/images/favicon_io/favicon-32x32.png" rel="icon">
  <link href="../assets/dist/css/bootstrap.min.css" rel="stylesheet">
  <link href="/css/styles.css" rel="stylesheet">
  <!-- font awesome script for icon used-->
  <script src="https://kit.fontawesome.com/d48db10d3a.js" crossorigin="anonymous"></script>
</head>

<body>
  <header>
    <nav class="navbar navbar-expand-md navbar-dark fixed-top bg-dark">
      <a class="navbar-brand" href="#"><img class="mb-4" src="/images/favicon_io/favicon.ico" alt="" width="72" height="72"></a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item">
            <a class="nav-link" href="#">DAILY DEALS</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">CONTACT</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">HELP</a>
          </li>
        </ul>
        <ul class="navbar-nav ml-auto">
          <li class="nav-item">
            <a class="nav-link" href="#">MY ACCOUNT</a>
          </li>
          <li class="nav-item">
            <span style="color: Dodgerblue;">
            <i class="fas fa-shopping-cart fa-3x"></i>
            </span>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">LOG OUT</a>
          </li>
        </ul>
      </div>
    </nav>
  </header>

  <main role="main">

    <div class="">
      <% for(var i=0;i<bookListItemsCount;i++) { %>
      <div class="row">
        <input type="checkbox">
        <svg class="bd-placeholder-img rounded-circle" width="140" height="140" xmlns="http://www.w3.org/2000/svg" preserveAspectRatio="xMidYMid slice" focusable="false" role="img" aria-label="Placeholder: 140x140">
          <title>Placeholder</title>
          <rect width="100%" height="100%" fill="#777" /><text x="50%" y="50%" fill="#777" dy=".3em">140x140</text>
        </svg>
        <p> <%= bookListTitle %> </p>
        <p> <%= bookListPrice %> </p>
      </div>
      <% } %>
    </div>

    <hr class="extra-margins">
  </main>
  <!-- FOOTER -->
  <div class="footer-padding"></div>
  <div class="footer">
    <p>©Copyright 2020 Al-Siddique ☪️ Bookstore</p>
  </div>
  </div>

</body>

</html>
